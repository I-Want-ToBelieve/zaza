{
  "name": "zaza",
  "version": "0.0.0",
  "description": "",
  "main": "src/init.ts",
  "type": "module",
  "workspaces": {
    "packages": [
      "pkgs/**/*"
    ]
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "one": "pnpm build:all && esno ./scripts/remove_this_args.ts && nvim +'hi NormalFloat guibg=#1e222a' +PackerInstall +PackerCompile",
    "one:clean": "pnpm build:all && esno ./scripts/remove_this_args.ts && pnpm clean && nvim +'hi NormalFloat guibg=#1e222a'",
    "build": "rm -rf ./lua && tstl",
    "build:all": " pnpm recursive run build && pnpm build",
    "clean": "rm -rf ~/.local/share/nvim/site/pack/packer/ && rm -rf plugin/",
    "watch": "pnpm build --watch",
    "start": "pnpm build",
    "commit": "git-cz",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm lint --fix",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [],
  "author": "backtolife",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@changesets/cli": "^2.24.4",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@microsoft/rush": "^5.79.0",
    "@prettier/plugin-lua": "^0.0.1",
    "@quitsmx/eslint-config": "^0.5.5",
    "@types/eslint": "^8.4.6",
    "@types/luaparse": "^0.2.8",
    "@types/node": "^18.7.20",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@typescript-to-lua/language-extensions": "^1.0.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.24.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettierx": "^0.18.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "esno": "^0.16.3",
    "husky": "^8.0.1",
    "lerna": "^5.5.2",
    "lint-staged": "^13.0.3",
    "lua-to-typescript": "^0.2.2",
    "lua-types": "^2.13.0",
    "luaparse": "^0.3.1",
    "prettier": "github:aMarCruz/prettier",
    "remark-parse": "^10.0.1",
    "replace-in-file": "^6.3.5",
    "supports-color": "^9.2.3",
    "ts-node": "^10.9.1",
    "turbo": "^1.5.3",
    "typescript": "^4.8.3",
    "typescript-to-lua": "^1.10.1",
    "zx": "^7.0.8"
  },
  "dependencies": {
    "@zaza/core": "workspace:^0.0.1",
    "@zaza/types": "workspace:^1.0.0",
    "@zaza/utils": "workspace:^0.0.1"
  },
  "volta": {
    "node": "16.17.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "typescript-to-lua@1.10.1": "patches/typescript-to-lua@1.10.1.patch",
      "lua-to-typescript@0.2.2": "patches/lua-to-typescript@0.2.2.patch"
    }
  }
}
